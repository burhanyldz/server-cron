NODE_ENV=development
MONGODB_URI=mongodb://localhost:27017
MONGODB_DB_NAME=server_commands
COMMAND_RUN_POLL_CRON=* * * * *
LOCK_TIMEOUT_MINUTES=10
COMMAND_TIMEOUT_MS=300000

# Optional wrapper for SYSTEM-level execution on Windows.
# Example:
# SYSTEM_COMMAND_WRAPPER=psexec -accepteula -s {command}
SYSTEM_COMMAND_WRAPPER=

# If true, steps with runAsSystem=true will fail when wrapper is not configured.
SYSTEM_COMMAND_STRICT=false
